{
  "displayName": "Guten",
  "publisher": "gutenye",
  "name": "guten",
  "version": "0.0.2",
  "description": "",
  "packageManager": "yarn@3.1.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "repository": "https://github.com/gutenye/vscode-extensions/tree/main/guten",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gutenye.makeExecutable"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gutenye.makeExecutable",
        "title": "Guten: Make Active File Executable"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "yarn vsce publish --no-dependencies",
    "package": "yarn vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vsce": "^2.5.1"
  }
}
